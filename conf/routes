# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index
POST     /test                       controllers.Application.test
POST     /testDB                       controllers.Application.testDB
GET    /testJ                      @controllers.ApplicationJava.index1()
POST   /auth/login                                 @controllers.ApplicationJava.login()

POST   /auth/customer/register                     @controllers.ApplicationJava.register()

POST   /auth/customer/spregister                     @controllers.ApplicationJava.spRegister()

GET   /history                                    @controllers.ApplicationJava.history()
# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)
